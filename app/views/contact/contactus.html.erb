<div class="row-fluid">
    <div class="span12">
        <div class="gmap"><iframe class="" width="100%" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/?ie=UTF8&ll=39.90403,116.407526&spn=1.118782,2.705383&t=m&z=10&output=embed"></iframe></div>
    </div>
</div>

<div class="row-fluid">
    <div class="span6">
        <div class="well">
            <section>
                <h3>Address</h3>
                <address>
                    <p>
                        <i class="icon-map-marker"></i> Street Name 432/2, BeiJing, 90210<br/>
                        <i class="icon-user"></i> (123) 456-7890<br/>
                        <i class="icon-print"></i> (123) 456-7890<br/>
                        <i class="icon-envelope"></i> <a href="mailto:#">bruce.yue@outlook.com</a>
                    </p>
                </address>
            </section>
            <section>
                <h3>Business Hour</h3>
                <ul class="unstyled">
                    <li class="clearfix">Monday - Fiyday:<p class="pull-right">9 am  to 6 pm</p></li>
                    <li class="clearfix">Saturday:<p class="pull-right">10 am  to 4 pm</p></li>
                    <li class="clearfix">Sunday:<p class="pull-right">Closed</p></li>
                </ul>
            </section>
        </div>
    </div>
    <div class="span6">
        <div class="well">
            <h3>Leave Your Message</h3>
            <form name="contact" method="post" action="contact.html" class="af-form" id="af-form">
                <div class="row-fluid">

                    <div class="af-outer af-required">
                        <div class="af-inner">
                            <input type="text" name="name" id="name" size="30" value="" placeholder="Name" class="text-input span12 placeholder" />
                            <label class="error" for="name" id="name_error">Name is required.</label>
                        </div>
                    </div>

                </div>
                <div class="row-fluid">

                    <div class="af-outer af-required">
                        <div class="af-inner">
                            <input type="text" name="email" id="email" size="30" value="" placeholder="Email" class="text-input span12 placeholder" />
                            <label class="error" for="email" id="email_error">Email is required.</label>
                        </div>
                    </div>

                </div>
                <div class="row-fluid">

                    <div class="af-outer af-required">
                        <div class="af-inner">
                            <textarea name="message" id="input-message" cols="20" placeholder="Message" class="text-input span12 placeholder"></textarea>
                            <label class="error" for="input-message" id="message_error">Message is required.</label>
                        </div>
                    </div>

                </div>
                <div class="row-fluid">

                    <div class="af-outer af-required">
                        <div class="af-inner">
                            <input type="submit" name="submit" class="form-button btn btn-primary btn-large" id="submit_btn" value="Send Message!" />
                        </div>
                    </div>

                </div>
            </form>
        </div>
    </div>
</div>